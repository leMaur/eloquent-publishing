{
    "name": "lemaur/laravel-model-publishing",
    "description": "",
    "keywords": [
        "lemaur",
        "laravel-model-publishing"
    ],
    "homepage": "https://github.com/lemaur/laravel-model-publishing",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Maurizio",
            "email": "hello@lemaur.me"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Lemaur\\Publishing\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "Lemaur\\Publishing\\PublishingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
